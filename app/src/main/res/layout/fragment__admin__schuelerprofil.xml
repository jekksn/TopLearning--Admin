        android:layout_alignEnd="@+id/textView59" />

    <TextView
        android:text="Mathe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:id="@+id/textView59"
        android:layout_below="@+id/textView58"
        android:layout_alignRight="@+id/imageView8"
        android:layout_alignEnd="@+id/imageView8" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/abc"
        android:layout_marginTop="47dp"
        android:id="@+id/imageView8"
        android:layout_below="@+id/textView53"
